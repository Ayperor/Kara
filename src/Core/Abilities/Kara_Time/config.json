{
    "name": "Time",
    "version": "0.0.1",
    "author": "Emile Clark-Boman",
    "description": "Kara Base Time Library",
    "main": "main.py",
    "requirements": "require.txt",
    "commands": {
        "time": {
            "target": "currentTime",
            "keywords": ["what time"]
        },
        "date": {
            "target": "currentDate",
            "keywords": ["what date"]
        },
        "month": {
            "target": "currentMonth",
            "keywords": ["what month"]
        },
        "year": {
            "target": "currentYear",
            "keywords": ["what year"]
        }
    }
}
