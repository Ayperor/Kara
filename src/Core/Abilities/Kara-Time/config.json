{
    "name": "Time",
    "version": "0.0.1",
    "author": "Emile Clark-Boman",
    "description": "Kara Base Time Library",
    "main": "main.py",
    "requirements": "require.txt",
    "commands": {
        "time": {
            "target": "currentTime",
            "keywords": ["time"]
        },
        "date": {
            "target": "currentDate",
            "keywords": ["date"]
        },
        "month": {
            "target": "currentMonth",
            "keywords": ["month"]
        },
        "year": {
            "target": "currentYear",
            "keywords": ["year"]
        }
    }
}
