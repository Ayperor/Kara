{
    "name": "Time",
    "version": "1.0.0",
    "author": "Emile Clark-Boman",
    "description": "Kara Base Time Library",
    "long-description": "",
    "main": "main.py",
    "requirements": "requirements.txt",
    "commands": {
        "time": {
            "target": "currentTime",
            "keywords": ["what time"]
        },
        "date": {
            "target": "currentDate",
            "keywords": ["what date"]
        },
        "month": {
            "target": "currentMonth",
            "keywords": ["what month"]
        },
        "year": {
            "target": "currentYear",
            "keywords": ["what year"]
        }
    }
